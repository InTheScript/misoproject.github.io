- @page_title = "The Miso Project :: Dataset :: API Reference"

%article.api

  = api_section('miso.dataset.yaml')

  %div.nav
    %ul
      - @api.each do |obj|
        %li
          %a
            = obj['name']

          - if obj['methods']
            %ul.object
              - obj['methods'].each do |method|
                %li
                  = method['name']

          - if obj['instance_methods']
            %span.instance
              Instance Methods
            %ul
              - obj['instance_methods'].each do |method|
                %li
                  %a
                    = method['name']


            
